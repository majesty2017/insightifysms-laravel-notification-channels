{
    "name": "insightifysms-laravel-notification-channels/insightifysms",
    "description": "Insightify SMS Notification Channel For Laravel",
    "homepage": "https://github.com/insightifysms-laravel-notification-channels/insightifysms",
    "license": "MIT",
    "authors": [
        {
            "name": "Reborn Dzre",
            "email": "reborndzre@gmail.com",
            "homepage": "https://insightifysms.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.0 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~5.0 || ~6.0 || ~7.0 || ~8.0",
        "insightifysms/insightifysms-api-sdk": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\InsightifySMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\InsightifySMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "extra": {
        "laravel": {
            "providers": [
              "InsightifySmsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
